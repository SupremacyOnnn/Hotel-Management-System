{
  "name": "hotelmanagementservice",
  "version": "1.0.0",
  "description": "Hotel Management System for a private hotel to kick start there online journey.",
  "type": "module",
  "main": "server.js",
  "scripts": {
    "start": "node backend/server.js",
    "server": "nodemon backend/server.js",
    "client": "npm start --prefix frontend",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "build": "npm install && npm install --prefix frontend && npm run build --prefix frontend"
  },
  "author": "Shubham Kumar Chowdhury",
  "license": "ISC",
  "dependencies": {
    "bcrypt": "^5.1.1",
    "bcryptjs": "^2.4.3",
    "concurrently": "^8.2.2",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dayjs": "^1.11.10",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.2",
    "moment": "^2.30.1",
    "mongoose": "^8.1.1",
    "nodemon": "^3.0.3",
    "react-date-range": "^2.0.0-alpha.4"
  },
  "devDependencies": {
    "dotenv": "^16.4.1"
  }
}
